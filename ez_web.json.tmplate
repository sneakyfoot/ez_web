{
    "recommends": "houdini_version >= '17.5.321'",

    "env": [
        {
            "EZ_WEB": "your/clone/dir"
        },
        {
            "PATH": {
                "method": "prepend",
                "value": [
                    "$EZ_WEB/bin"
                ]
            }
        }
    ],
    "path": "$EZ_WEB"
}
